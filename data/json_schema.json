
{
  "type": "object",
  "properties": {
    "Graph": {
      "type": "array"
    },
    "algorithm_data": {
      "type": "object",
      "properties": {
        "n_pop": {
          "type": "integer",
          "minimum": 1
        },
        "n_iterations": {
          "type": "integer",
          "minimum": 1
        },
        "penalty_factor": {
          "type": "number",
          "exclusiveMinimum": 0
        },
        "r_mutation": {
          "type": "number",
          "minimum": 0,
          "maximum": 1
        },
        "parent_percent": {
          "type": "number",
          "minimum": 0,
          "maximum": 100
        },
        "uncomplete_sol": {
          "type": "boolean"
        },
        "selection_type": {
          "type": "string",
          "enum": ["selection", "selection_tour", "selection_roulette", "selection_rank"]
        }
      }
    },
    "order_lst": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "n_pallets": {
            "type": "integer",
            "minimum": 1
          },
          "vertex": {
            "type": "number",
            "minimum": 0
          },
          "deadline": {
            "type": "number",
            "minimum": 0
          },
          "index": {
            "type": "integer",
            "minimum": 0
          }
        }
      }
    },
    "structures_lst": {
      "type": "object",
      "properties": {
        "SIMULATION_TIME": {
          "type": "number",
          "minimum": 1
        },
        "rows_cols": {
          "type": "integer",
          "minimum": 1
        },
        "low_adj_matrix": {
          "type": "number",
          "exclusiveMinimum": 0
        },
        "high_adj_matrix": {
          "type": "number",
          "exclusiveMinimum": 0
        },
        "n_of_orders": {
          "type": "integer",
          "minimum": 1
        },
        "max_pallets": {
          "type": "integer",
          "minimum": 1
        },
        "n_small_trucks": {
          "type": "integer",
          "minimum": 0
        },
        "n_large_trucks": {
          "type": "integer",
          "minimum": 0
        },
        "speed_s": {
          "type": "number",
          "exclusiveMinimum": 0
        },
        "capacity_s": {
          "type": "integer",
          "minimum": 1
        },
        "speed_l": {
          "type": "number",
          "exclusiveMinimum": 0
        },
        "capacity_l": {
          "type": "integer",
          "minimum": 1
        }
      }
    },
    "truck_list": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "enum": ["TruckType.SMALL","TruckType.LARGE"]
          },
          "index": {
            "type": "integer",
            "minimum": 0
          }
        }
      }
    }
  }
}